<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activites.FundingCompanies"
    tools:showIn="@layout/app_bar_funding_companies"
    android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/funding_companies"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:textSize="20dp"
            android:paddingLeft="10dp"
            android:paddingStart="10dp"

            android:fontFamily="@font/droid"
            android:paddingRight="10dp"
            android:paddingEnd="10dp"
            android:textColor="@color/white"
            android:background="@color/blue_subscribtion_button"/>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/fram">

        </FrameLayout>
    </LinearLayout>
